/* 搜索框 */

/* 无结果 */
.none{
  margin: auto;
  width: 500rpx;
  font-size: 40px;
  text-align: center;
}

scroll-view{
  background-color: white;
}
/* 医生列表 */
.floor{
  border-bottom: solid #e9e9e9;
  width: 100%;
  position: relative;
}

.floor .info{
  width: 100%;
  height:200rpx;
}

.floor .info .name{
  font-size: large;
  font-weight: bold;
}

.floor .info .title{
  border: none;
  margin-left: 10rpx;
}

.floor .info .excel{
  height: 100rpx;
  color:#3d3d3d;
  overflow: hidden;
  /*超出隐藏*/
  text-overflow: ellipsis;
  /*超出显示为省略号*/
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.group {
  display: flex;
  flex-wrap: wrap;
}
.group .image_item {
  width: 200rpx;
  height: 200rpx;
  margin-right: 10rpx;
  position: relative;
}
button{
  display: inline-block;
  background-color: rgb(105, 153, 255);
}

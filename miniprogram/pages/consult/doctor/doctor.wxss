/* 顶部切换 */
.navbar{
  flex: none;
  display: flex;
  background: #fff;
  font-size: 30rpx;
  color: #bbbbbb;
}
.navbar .item{
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 90rpx;
}
.navbar .item.active{
  color: #908df5;
}
.navbar .item.active:after{
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 5rpx;
  background: #908df5;
}

/* 医生列表 */
.doctor_floor{
  border-bottom: solid #e9e9e9;
  display: flex;
  width: 100%;
  height: 220rpx;
  position: relative;
}

.doctor_floor .background{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: -1;
}

.doctor_floor .avatar{
  width: 100rpx;
  height:100rpx;
  margin:10rpx
}

.doctor_floor .avatar text{
  font-size: 80rpx;
}

